<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="devsb.LeaderboardController">
   <children>
      <TableView fx:id="leaderboardTable" layoutX="155.0" layoutY="53.0" prefHeight="200.0" prefWidth="385.0">
        <columns>
          <TableColumn fx:id="pos" prefWidth="75.0" text="Position" />
          <TableColumn fx:id="name" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="rollNo" prefWidth="75.0" text="Roll Number" />
            <TableColumn fx:id="score" prefWidth="75.0" text="Score" />
            <TableColumn fx:id="timeLeft" prefWidth="75.0" text="Time Left" />
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="173.0" layoutY="301.0" mnemonicParsing="false" onAction="#onBackClicked" text="Back" />
   </children>
</AnchorPane>
